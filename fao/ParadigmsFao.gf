		<_ + "ða", _ + "ur"> => mkV015 form1;
		<_ + "ma", _ + "ar"> => mkV001 form1;
		<_ + "a", _ + "t"> => mkV027 form1;
		_ => regV form1
  } ;

mkN = overload {
  mkN : Str -> N = regN;   -- lemma
  mkN : Str -> Str -> N = reg2N   -- lemma  Indef;Pl;Nom
} ;

mkA = overload {
  mkA : Str -> A = regA;   -- lemma
