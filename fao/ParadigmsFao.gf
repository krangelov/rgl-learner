  reg2V : Str -> Str -> V   -- lemma  Indicative;Pres;P2;Sg
    = \form1, form2 -> case <form1, form2> of {
		<_ + "gva", _ + "ørt"> => mkV079 form1;
		<_ + "ala", _ + "ar"> => mkV001 form1;
